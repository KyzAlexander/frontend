{
  "extends": ["next", "next/core-web-vitals"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["error"],
    "no-unused-vars": "off",
    "no-use-before-define": "error",
    "no-console": "warn",
    "array-bracket-spacing": "error",
    "indent": ["error", 2]
  }
}
